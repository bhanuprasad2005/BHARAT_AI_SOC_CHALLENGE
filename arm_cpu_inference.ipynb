#Cell 3 — Define CNN Layers (NumPy Implementation)

def relu(x):
    return np.maximum(0, x)

def sigmoid(x):
    return 1 / (1 + np.exp(-x))

def conv2d(input_img, kernel, bias):
    h, w, c = input_img.shape
    kh, kw, _, filters = kernel.shape

    output = np.zeros((h - kh + 1, w - kw + 1, filters))

    for f in range(filters):
        for i in range(h - kh + 1):
            for j in range(w - kw + 1):
                region = input_img[i:i+kh, j:j+kw, :]
                output[i, j, f] = np.sum(region * kernel[:, :, :, f]) + bias[f]

    return output

def maxpool(input_img, size=2):
    h, w, c = input_img.shape
    out_h = h // size
    out_w = w // size

    output = np.zeros((out_h, out_w, c))

    for k in range(c):
        for i in range(out_h):
            for j in range(out_w):
                region = input_img[i*size:(i+1)*size, j*size:(j+1)*size, k]
                output[i, j, k] = np.max(region)

    return output


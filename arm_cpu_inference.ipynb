# Conv Layer 2
x = conv2d(x, W2, b2)
x = relu(x)
x = maxpool(x)

# Flatten
x = x.flatten()

# Dense 1
x = relu(np.dot(x, W3) + b3)

# Output
output = sigmoid(np.dot(x, W4) + b4)

end = time.time()

print("Prediction:", output)
print("Inference Time (seconds):", end - start)
